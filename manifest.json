{
  "manifest_version": 2,

  "name": "Bilibili扩展(AOPOD)",
  "description": "aopod.com",
  "version": "1.0.0",

  "content_scripts": [{
    "css": ["default/default.css"],
    "js": ["default/default.js"],
    "matches": [
      "http://*.bilibili.com/*",
      "https://*.bilibili.com/*",
      "http://*.bilibili.tv/*",
      "https://*.bilibili.tv/*"
    ]
  }]
}
